# This specifies the base Ubuntu system
BASEIMAGE_URL=https://rcn-ee.com/rootfs/debian-armhf-11-bullseye-iot-v5.10-ti/2023-12-16/am335x-debian-11.8-iot-armhf-2023-12-16-4gb.img.xz
BASEIMAGE=`basename ${BASEIMAGE_URL}`



ANSIBLE_PLATFORM_VARS='{"platform":"replicape", "packaging":"apt"}'

function decompress {
	xz -c -d $BASEIMAGE >> $TARGETIMAGE
}
