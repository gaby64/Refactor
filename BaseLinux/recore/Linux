# This specifies the base image system
BASEIMAGE_URL=https://github.com/intelligent-agent/Rebuild/releases/download/v0.0.1-RC5/rebuild-barebone-v0.0.1-RC5.img.xz

BASEIMAGE=rebuild-barebone-v0.0.1-RC5.img.xz

ANSIBLE_PLATFORM_VARS='{"platform":"recore", "packaging":"apt", "ansible_python_interpreter": "/usr/bin/python3"}'

function decompress {
	xz -T0 -vc -c -d $BASEIMAGE >> $TARGETIMAGE
}
